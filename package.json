{
  "name": "@fomolol/dope",
  "version": "0.0.2",
  "description": "A collection of dope React components that we like to use.",
  "files": [
    "dist"
  ],
  "sideEffects": true,
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "scripts": {
    "dev": "vite docs",
    "size": "size-limit",
    "clean": "rimraf dist && rimraf bundled",
    "build": "npm run clean && npm run build:light && npm run build:bundle",
    "build:light": "microbundle",
    "build:bundle": "microbundle --external none --output './bundled'",
    "build:docs": "vite build",
    "prebuild": "npm run clean",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fomolol/dope.git"
  },
  "keywords": [],
  "author": "@studio-freight",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fomolol/modulemaker/issues"
  },
  "homepage": "https://github.com/fomolol/dope#readme",
  "devDependencies": {
    "@fomolol/tacklebox": "^0.1.1",
    "@react-three/drei": "^9.46.0",
    "@react-three/fiber": "^8.9.1",
    "@size-limit/preset-small-lib": "^8.0.1",
    "clsx": "^1.2.1",
    "framer-motion": "^7.6.15",
    "gl-noise": "^1.6.1",
    "iphone-inline-video": "^2.2.2",
    "leva": "^0.9.34",
    "lodash": "^4.17.21",
    "microbundle": "^0.15.1",
    "next": "^13.0.5",
    "node-sass": "^8.0.0",
    "paper": "^0.12.17",
    "path": "^0.12.7",
    "react-device-detect": "^2.2.2",
    "react-howler": "^5.2.0",
    "react-intersection-observer": "^9.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "size-limit": "^8.0.1",
    "three": "^0.147.0",
    "three-custom-shader-material": "^5.0.0",
    "use-fit-text": "^2.4.0",
    "use-sound": "^4.0.1",
    "vite": "^3.0.9"
  },
  "size-limit": [
    {
      "limit": "3 kB",
      "path": "dist/index.js"
    },
    {
      "limit": "2 kB",
      "path": "dist/index.mjs"
    }
  ],
  "peerDependencies": {
    "@fomolol/tacklebox": "^0.1.1",
    "@react-three/drei": "^9.46.0",
    "@react-three/fiber": "^8.9.1",
    "clsx": "^1.2.1",
    "framer-motion": "^7.6.15",
    "gl-noise": "^1.6.1",
    "iphone-inline-video": "^2.2.2",
    "leva": "^0.9.34",
    "lodash": "^4.17.21",
    "next": "^13.0.5",
    "node-sass": "^8.0.0",
    "paper": "^0.12.17",
    "react-device-detect": "^2.2.2",
    "react-howler": "^5.2.0",
    "react-intersection-observer": "^9.4.1",
    "sass": "^1.56.1",
    "three": "^0.147.0",
    "three-custom-shader-material": "^5.0.0",
    "use-fit-text": "^2.4.0",
    "use-sound": "^4.0.1"
  },
  "directories": {
    "doc": "docs"
  }
}
