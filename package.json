  "description": "A collection of dope React components that we like to use.",
  "files": [
    "dist"
  ],
  "sideEffects": true,
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "scripts": {
    "dev": "vite docs",
    "size": "size-limit",
    "clean": "rimraf dist && rimraf bundled",
    "build": "npm run clean && npm run build:light && npm run build:bundle",
    "build:light": "microbundle",
    "build:bundle": "microbundle --external none --output './bundled'",
    "build:docs": "vite build",
    "prebuild": "npm run clean",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider STORYBOOK_BASE_HREF=/storybook/ build-storybook -o ./storybook",
    "serve-storybook": "serve storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fomolol/dope.git"
  },
